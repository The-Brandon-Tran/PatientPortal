            <Font size="27.0" />
         </font>
      </Label>
      <ComboBox fx:id="roleCombo" layoutX="152.0" layoutY="119.0" onMouseClicked="#setRoleItems" prefHeight="41.0" prefWidth="200.0" promptText="Select Role" visibleRowCount="2" />
      <TextField fx:id="tfUserName" layoutX="152.0" layoutY="187.0" prefHeight="41.0" prefWidth="200.0" promptText="Username" />
      <TextField fx:id="tfPassword" layoutX="152.0" layoutY="255.0" prefHeight="41.0" prefWidth="200.0" promptText="Password" />
